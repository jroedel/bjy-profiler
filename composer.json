{
    "name": "rafalli/bjy-profiler",
    "description": "Database profiler for Laminas\\Db. Module forked from andrey-mokhov/bjy-profiler",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laminas",
        "db",
        "profiler"
    ],
    "homepage": "https://github.com/rafalli/BjyProfiler",
    "authors": [
        {
            "name": "Ben Youngblood",
            "email": "bx.youngblood@gmail.com",
            "homepage": "http://bjyoungblood.com/"
        },
        {
            "name": "Andrey N. Mokhov",
            "email": "github@mokhov.com",
            "homepage": "https://github.com/andrey-mokhov"
        },
        {
            "name": "Rafal Linca",
            "email": "kontakt@rafalllinca.pl",
            "homepage": "https://rafallinca.pl"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0 || ^8.0",
        "laminas/laminas-servicemanager": "^3.0.3",
        "laminas/laminas-modulemanager": "^2.7",
        "laminas/laminas-db": "^2.7",
        "laminas/laminas-log": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "BjyProfiler\\": "src/"
        }
    },
    "extra": {
        "zf": {
            "module": "BjyProfiler",
            "config-provider": "BjyProfiler\\ConfigProvider"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 || ^5.0",
        "laminas/laminas-mvc": "^2.7 || ^3.0"
    }
}
